<?xml version="1.0" ?>
<component type="desktop">
  <id>kaudiocreator.desktop</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-2+</project_license>
  <name>KAudioCreator</name>
  <summary>CD Ripper</summary>
  <description>
    <p>
      KAudioCreator is an audio CD ripper, which uses &quot;cdparanoia&quot; as a backend to extract the tracks
      and later it encodes them to mp3 or ogg files (or many other formats supported)
    </p>
    <p>Features:</p>
    <ul>
      <li>CDDB lookup to extract the album and track information</li>
      <li>Different encoding formats supported (ogg, mp3, flac)</li>
      <li>Different filename formats using tags</li>
    </ul>
  </description>
  <url type="bugtracker">https://bugs.kde.org/enter_bug.cgi?format=guided&amp;product=kaudiocreator</url>
  <url type="help">http://docs.kde.org/development/en/extragear-multimedia/kaudiocreator/index.html</url>
  <screenshots>
    <screenshot type="default">
      <image>http://kde.org/images/screenshots/kaudiocreator.png</image>
    </screenshot>
  </screenshots>
  <project_group>KDE</project_group>
  <provides>
    <binary>kaudiocreator</binary>
  </provides>
</component>
