    <connection>
        <sender>playlistFormat</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>fileWizard</receiver>
        <slot>fileFormatTextChanged(const QString&amp;)</slot>
    </connection>
    <connection>
        <sender>songArtistButton</sender>
        <signal>clicked()</signal>
        <receiver>fileWizard</receiver>
        <slot>songArtistPressed()</slot>
    </connection>
    <connection>
        <sender>songCommentButton</sender>
        <signal>clicked()</signal>
